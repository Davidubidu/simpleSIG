{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "lib": ["es2018", "dom"],
    "paths": {
      "@interceptors": ["src/app/interceptors/index.ts"],
      "@services": ["src/app/services/index.ts"],
      "@admin": ["src/app/admin/index.ts"],
      "@admin/*": ["src/app/admin/*"],
      "@auth": ["src/app/auth/index.ts"],
      "@auth/*": ["src/app/auth/*"],
      "@map": ["src/app/map/index.ts"],
      "@map/*": ["src/app/map/*"],
      "@settings": ["src/app/settings/index.ts"],
      "@settings/*": ["src/app/settings/*"],
      "@components": ["src/app/components/index.ts"],
      "@models": ["src/app/models/index.ts"],
      "@stores": ["src/app/stores/index.ts"]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}
